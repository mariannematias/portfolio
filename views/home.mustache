<html lang="en" class="no-js">
    <!-- BEGIN HEAD -->
    {{>head}}
    <!-- END HEAD -->

    <!-- BODY -->
    <body id="body" data-spy="scroll" data-target=".header">

        <!--========== HEADER ==========-->
        <header class="header navbar-fixed-top">
            <!-- Navbar -->
            <nav class="navbar" role="navigation">
                <div class="container">
                    <!-- Brand and toggle get grouped for better mobile display -->
                    <div class="menu-container js_nav-item">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".nav-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="toggle-icon"></span>
                        </button>

                        <!-- Logo -->
                        <div class="logo">
                            <a class="logo-wrap" href="#body">
                                <img class="logo-img logo-img-main" src="{{{global.img_logo_header}}}" alt="Logo">
                                <img class="logo-img logo-img-active" src="{{{global.img_logo_footer}}}" alt="Logo">
                            </a>
                        </div>
                        <!-- End Logo -->
                    </div>

                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse nav-collapse">
                        <div class="menu-container">
                            <ul class="nav navbar-nav navbar-nav-right">
								{{#global.navigation_menu}}
									<li class="js_nav-item nav-item"><a class="nav-item-child nav-item-hover" href="{{url}}">{{text}}</a></li>
								{{/global.navigation_menu}}
                            </ul>
                        </div>
                    </div>
                    <!-- End Navbar Collapse -->
                </div>
            </nav>
            <!-- Navbar -->
        </header>
        <!--========== END HEADER ==========-->

        <!--========== SLIDER ==========-->
        <div class="promo-block parallax-window" data-parallax="scroll" data-image-src="{{{global.img_hero_bg}}}">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="promo-block-divider">
                            <h1 class="promo-block-title">{{global.profile.firstName}} <br/> {{global.hero.lastName}}</h1>
                            <p class="promo-block-text">{{global.profile.position}}</p>
                        </div>
                        <ul class="list-inline">
							{{#page.social.facebook.show_or_hide}}
                           		<li><a href="#" class="social-icons"><i class="icon-social-{{page.social.facebook.name}}"></i></a></li>
							{{/page.social.facebook.show_or_hide}}

                            {{#page.social.twitter.show_or_hide}}
                           		<li><a href="#" class="social-icons"><i class="icon-social-{{page.social.twitter.name}}"></i></a></li>
							{{/page.social.twitter.show_or_hide}}

                            {{#page.social.dribbble.show_or_hide}}
                           		<li><a href="#" class="social-icons"><i class="icon-social-{{page.social.dribbble.name}}"></i></a></li>
							{{/page.social.dribbble.show_or_hide}}

                            {{#page.social.pinterest.show_or_hide}}
                           		<li><a href="#" class="social-icons"><i class="icon-social-{{page.social.pinterest.name}}"></i></a></li>
							{{/page.social.pinterest.show_or_hide}}

                            {{#page.social.linkedin.show_or_hide}}
                           		<li><a href="#" class="social-icons"><i class="icon-social-{{page.social.linkedin.name}}"></i></a></li>
							{{/page.social.linkedin.show_or_hide}}
                            
                            {{#page.social.youtube.show_or_hide}}
                           		<li><a href="#" class="social-icons"><i class="icon-social-{{page.social.youtube.name}}"></i></a></li>
							{{/page.social.youtube.show_or_hide}}

                            {{#page.social.instagram.show_or_hide}}
                           		<li><a href="#" class="social-icons"><i class="icon-social-{{page.social.instagram.name}}"></i></a></li>
							{{/page.social.instagram.show_or_hide}}
                        </ul>
                    </div>
                </div>
                <!--// end row -->
            </div>
        </div>
        <!--========== SLIDER ==========-->

        <!--========== PAGE LAYOUT ==========-->
        <!-- About -->
        <div id="about">
            <div class="container content-lg">
                <div class="row">
                    <div class="col-sm-3 sm-margin-b-30">
                        <div class="text-right sm-text-left">
                            <h2 class="margin-b-0">{{page.intro_section.title}}</h2>
                            <p>{{page.intro_section.sub}}</p>
                        </div>
                    </div>
                    <div class="col-sm-8 col-sm-offset-1">
                        <div class="margin-b-60">
                            <p>{{page.intro_section.description}}</p>
                            <p>{{page.intro_section.description2}}</p>
                        </div>

                        <!-- Progress Box -->
						{{#page.intro_section.progress_bar}}
							<div class="progress-box">
								<h5>{{text}} <span class="color-heading pull-right">{{percent_text}}</span></h5>
								<div class="progress">
									<div class="progress-bar bg-color-base" role="progressbar" data-width="{{width}}"></div>
								</div>
                        	</div>
						{{/page.intro_section.progress_bar}}
                        <!-- End Progress Box -->
                    </div>
                </div>
                <!--// end row -->
            </div>
        </div>
        <!-- End About -->

        <!-- Experience -->
        <div id="experience">
            <div class="bg-color-sky-light" data-auto-height="true">
                <div class="container content-lg">
                    <div class="row">
                        <div class="col-sm-3 sm-margin-b-30">
                            <div class="text-right sm-text-left">
                                <h2 class="margin-b-0">{{page.experience_section.title}}</h2>
                                <p>{{page.experience_section.desc}}</p>
                            </div>
                        </div>
                        <div class="col-sm-8 col-sm-offset-1">
                            <div class="row row-space-2 margin-b-4">
								{{#page.experience_section.skill_list}}
									<div class="col-md-4 md-margin-b-4">
										<div class="service skill-box" data-height="height">
											<div class="service-element">
												<i class="service-icon icon-chemistry"></i>
											</div>
											<div class="service-info">
												<h3>{{text}}</h3>
												<p class="margin-b-5">{{description}}</p>
											</div>
											<a href="#" class="content-wrapper-link"></a>    
										</div>
									</div>
								{{/page.experience_section.skill_list}}
                            </div>
                            <!--// end row -->
                        </div>
                    </div>
                    <!--// end row -->
                </div>
            </div>
        </div>
        <!-- End Experience -->

        <!-- Work -->
        <div id="work">
            <div class="container content-lg">
                <div class="row">
                    <div class="col-sm-3 sm-margin-b-30">
                        <div class="text-right sm-text-left">
                            <h2 class="margin-b-0">{{page.work_section.title}}</h2>
                            <p>{{page.work_section.sub}}</p>
                        </div>
                    </div>
                    <div class="col-sm-8 col-sm-offset-1">
                        <!-- Masonry Grid -->
                        <div class="masonry-grid row row-space-2">
                            <div class="masonry-grid-sizer col-xs-6 col-sm-6 col-md-1"></div>

							{{#projects}}
								<div class="masonry-grid-item col-xs-12 col-sm-6 col-md-4">
									<!-- Work -->
									<div class="work work-popup-trigger">
										<div class="work-overlay">
											<img class="full-width img-responsive" src="{{{image}}}=s220" alt="Portfolio Image">
										</div>
										<div class="work-popup-overlay">
											<div class="work-popup-content">
												<a href="javascript:void(0);" class="work-popup-close">Hide</a>
												<div class="margin-b-30">
													<h3 class="margin-b-5">{{name}}</h3>
												</div>
												<div class="row">
													<div class="col-sm-8 work-popup-content-divider sm-margin-b-20">
														<div class="margin-t-10 sm-margin-t-0">
															<p>{{description}}</p>
														</div>
													</div>
												</div>
											</div>
										</div>
									</div>
									<!-- End Work -->
								</div>
							{{/projects}}
                        </div>
                        <!-- End Masonry Grid -->
                    </div>
                </div>
                <!--// end row -->
            </div>
        </div>
        <!-- End Work -->
            
        <!-- Contact -->
        <div id="contact">
            <div class="bg-color-sky-light">
                <div class="container content-lg">
                    <div class="row">
                        <div class="col-sm-3 sm-margin-b-30">
                            <div class="text-right sm-text-left">
                                <h2 class="margin-b-0">{{page.contact_section.title}}</h2>
                                <p>{{page.contact_section.sub}}</p>
                            </div>
                        </div>
                        <div class="col-sm-8 col-sm-offset-1">
                            <div class="row">
                                <div class="col-md-3 col-xs-6 md-margin-b-30">
                                    <h5>{{page.contact_section.address.label}}</h5>
                                    <span>{{page.contact_section.address.text}}</span>
                                </div>
                                <div class="col-md-3 col-xs-6 md-margin-b-30">
                                    <h5>{{page.contact_section.phone.label}}</h5>
                                    <span>{{page.contact_section.phone.text}}</span>
                                </div>
                                <div class="col-md-3 col-xs-6 md-margin-b-30">
                                    <h5>{{page.contact_section.email.label}}</h5>
                                    <span>{{page.contact_section.email.text}}</span>
                                </div>
                                <div class="col-md-3 col-xs-6 md-margin-b-30">
                                    <h5>{{page.contact_section.website.label}}</h5>
                                    <span>{{page.contact_section.website.text}}</span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--// end row -->
                </div>
            </div>
        </div>
        <!-- End Contact -->
        <!--========== END PAGE LAYOUT ==========-->

        <!--========== FOOTER ==========-->
        <footer class="footer">
            <div class="content container">
                <div class="row">
                    <div class="col-xs-6">
                        <img class="footer-logo" src="{{{global.img_logo_footer}}}" alt="Aircv Logo">
                    </div>
                    <div class="col-xs-6 text-right sm-text-left">
                        <p class="margin-b-0"><a class="fweight-700" href="http://keenthemes.com/preview/aircv/">Aircv</a> Theme Powered by: <a class="fweight-700" href="http://www.keenthemes.com/">KeenThemes.com</a></p>
                    </div>
                </div>
                <!--// end row -->
            </div>
        </footer>
        <!--========== END FOOTER ==========-->

        <!-- Back To Top -->
        <a href="javascript:void(0);" class="js-back-to-top back-to-top">Top</a>

        <!-- Vertex library for enabling site updates from https://www.vertex360.co -->
        <script type="text/javascript" src="https://cdn.turbo360-dev.com/dist/vertex.min.js"></script>

        <!-- JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) -->
        <!-- CORE PLUGINS -->
        <script src="vendor/jquery.min.js" type="text/javascript"></script>
        <script src="vendor/jquery-migrate.min.js" type="text/javascript"></script>
        <script src="vendor/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>

        <!-- PAGE LEVEL PLUGINS -->
        <script src="vendor/jquery.easing.js" type="text/javascript"></script>
        <script src="vendor/jquery.back-to-top.js" type="text/javascript"></script>
        <script src="vendor/jquery.smooth-scroll.js" type="text/javascript"></script>
        <script src="vendor/jquery.wow.min.js" type="text/javascript"></script>
        <script src="vendor/jquery.parallax.min.js" type="text/javascript"></script>
        <script src="vendor/jquery.appear.js" type="text/javascript"></script>
        <script src="vendor/masonry/jquery.masonry.pkgd.min.js" type="text/javascript"></script>
        <script src="vendor/masonry/imagesloaded.pkgd.min.js" type="text/javascript"></script>

        <!-- PAGE LEVEL SCRIPTS -->
        <script src="js/layout.min.js" type="text/javascript"></script>
        <script src="js/components/progress-bar.min.js" type="text/javascript"></script>
        <script src="js/components/masonry.min.js" type="text/javascript"></script>
        <script src="js/components/wow.min.js" type="text/javascript"></script>

    </body>
    <!-- END BODY -->
</html>